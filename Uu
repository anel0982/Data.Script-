More:AddButton({
	Name = "Gui Tp player",
	Callback = function()
      		 local Gui = Instance.new("ScreenGui")
local BackGround = Instance.new("Frame")
local PlayerTextBox = Instance.new("TextBox")
local ExitGuiButton = Instance.new("TextButton")
local TeleportButton = Instance.new("ImageButton")
