BackGround.Name = "BackGround"
BackGround.Parent = Gui
BackGround.Active = true
BackGround.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
BackGround.BorderColor3 = Color3.fromRGB(27, 42, 53)
BackGround.BorderSizePixel = 0
